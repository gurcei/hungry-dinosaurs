      IF FILLØSTATE%<>0 THEN GOSUB BOXFILL:ELSE BOX BX0%, BY0%, MX%, MY%      LOADØSTATE% = -1      GOTO NXTØLP    BEND    IF LOADØSTATE% = –œÃŸ THEN BEGIN      PRINT "POLY ";      GET#2,K:K = K AND 255      POLYØIDX% = K      FOR K = 0 TO POLYØIDX%-1        GET#2,M0% : M0% = M0% AND 255        GET#2,M1% : M1% = M1% AND 255        POLYØX%(K) = M0% + (M1% << 8)        GET#2,M0% : M0% = M0% AND 255        GET#2,M1% : M1% = M1% AND 255        POLYØY%(K) = M0% + (M1% << 8) + OFFSET%        IF POLYØY%(K)<0 THEN BEGIN:POLYØY%(K)=0:BEND        IF (K>0) THEN LINE POLYØX%(K-1), POLYØY%(K-1), POLYØX%(K), POLYØY%(K)      NEXT K      LINE POLYØX%(POLYØIDX%-1),POLYØY%(POLYØIDX%-1),POLYØX%(0),POLYØY%(0)      IF FILLØSTATE% = 1 THEN GOSUB FILLØPOLY      LOADØSTATE% = -1      GOTO NXTØLP    BEND.NXTØLP  ' GOSUB POLLØPLAY  LOOP  DCLOSE #2  IF HASØATTICØRAM% AND ONION%=0 THEN BEGIN    EDMA 0, 0, $40000, $8000000 + $20000*CURRENTØROOM%    EDMA 0, 0, $50000, $8000000 + $20000*CURRENTØROOM% + $10000    CACHED%(CURRENTØROOM%) = 1  BEND  ONION% = 0  RETURN.INITØMUSIC'==========  IF S% <> CURRENTØROOM% THEN BEGIN    S% = CURRENTØROOM%    PLAY:PLAY "","","","","",""    FOR K = 0 TO 10     V1$(K)="":V2$(K)="":V3$(K)="":V4$(K)="":V5$(K)="":V6$(K)=""    NEXT K    TEMPO SONGSØTMPO%(S%)    BARØINDEX=0    S% = CURRENTØROOM%    IF S% = 0 THEN GOSUB GERMANØSONG    IF S% = 1 THEN GOSUB TURKØSONG    IF S% = 2 THEN GOSUB THAIØSONG    IF S% = 3 THEN GOSUB VIETØSONG    IF S% = 4 THEN GOSUB AUSØSONG  BEND  RETURN.POLLØPLAY'---------  IF RPLAY(1) THEN RETURN  MUSICØCHUNKØINDEX=BARØINDEX ' MAPØBARØTOØCHUNK(BARØINDEX)  IF S% = ‘’“À≈Ÿ THEN MUSICØCHUNKØINDEX=MAP(BARØINDEX)    IF SONGSØCPY%(S%)=1 THEN BEGIN    PLAY V1$(MUSICØCHUNKØINDEX),V2$(MUSICØCHUNKØINDEX),V3$(MUSICØCHUNKØINDEX)    SLEEP .05    PLAY ,,,V1$(MUSICØCHUNKØINDEX),V2$(MUSICØCHUNKØINDEX),V3$(MUSICØCHUNKØINDEX)  BEND:ELSE BEGIN    PLAY V1$(MUSICØCHUNKØINDEX),V2$(MUSIC